@model IEnumerable<Amici.DA.Models.Amico>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width"/>
    <title>@ViewBag.Title</title>
</head>
<body>
<div>
    @foreach (var amico in Model) {
        <div>
            <h3>@amico.Nome</h3>
            <h3>@amico.Cognome</h3>
        </div>
    }
</div>
</body>
</html>
